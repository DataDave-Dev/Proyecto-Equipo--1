   <string notr="true">
/* === FORM GLOBAL === */
QWidget#UserForm {
    background-color: transparent;
}

/* === LABELS === */
QLabel#form_titulo {
    font-size: 24px;
    font-weight: bold;
    color: #1a1a2e;
}
QLabel#form_subtitulo {
    font-size: 13px;
    color: #718096;
}
QLabel#seccionPersonal, QLabel#seccionCuenta, QLabel#seccionPermisos {
    font-size: 15px;
    font-weight: bold;
    color: #1a1a2e;
    padding-top: 8px;
}
QLabel.fieldLabel {
    font-size: 13px;
    color: #2d3748;
    font-weight: 600;
}

/* === INPUTS === */
QLineEdit, QComboBox {
    padding: 8px 12px;
    border: 1px solid #e2e8f0;
    border-radius: 6px;
    background-color: #f7fafc;
    font-size: 13px;
    min-height: 34px;
}
QLineEdit:focus, QComboBox:focus {
    border: 1px solid #4a90d9;
    background-color: white;
}
QLineEdit:hover, QComboBox:hover {
    border: 1px solid #cbd5e0;
}

/* === CHECKBOX === */
QCheckBox#check_activo {
    font-size: 13px;
    color: #2d3748;
    spacing: 8px;
}
QCheckBox#check_activo::indicator {
    width: 18px;
    height: 18px;
    border: 2px solid #e2e8f0;
    border-radius: 4px;
    background-color: #f7fafc;
}
QCheckBox#check_activo::indicator:checked {
    background-color: #4a90d9;
    border-color: #4a90d9;
}
QCheckBox#check_activo::indicator:hover {
    border-color: #cbd5e0;
}

/* === BUTTONS === */
QPushButton#btn_guardar {
    background-color: #4a90d9;
    color: white;
    border: none;
    border-radius: 6px;
    font-size: 14px;
    font-weight: 600;
    padding: 10px 24px;
    min-height: 38px;
}
QPushButton#btn_guardar:hover {
    background-color: #3a7bc8;
}
QPushButton#btn_guardar:pressed {
    background-color: #2a6bb8;
}
QPushButton#btn_limpiar, QPushButton#btn_cancelar {
    background-color: white;
    color: #2d3748;
    border: 1px solid #e2e8f0;
    border-radius: 6px;
    font-size: 14px;
    font-weight: 500;
    padding: 10px 20px;
    min-height: 38px;
}
QPushButton#btn_limpiar:hover, QPushButton#btn_cancelar:hover {
    background-color: #f7fafc;
    border-color: #cbd5e0;
}
QPushButton#btn_toggle_password, QPushButton#btn_toggle_confirm {
    background-color: #f0f2f5;
    border: 1px solid #e2e8f0;
    border-radius: 6px;
    font-size: 14px;
}
QPushButton#btn_toggle_password:hover, QPushButton#btn_toggle_confirm:hover {
    background-color: #e2e8f0;
    border: 1px solid #cbd5e0;
}

/* === SEPARATOR === */
QFrame#separator1, QFrame#separator2, QFrame#separator3 {
    color: #e2e8f0;
}
